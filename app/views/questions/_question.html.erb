<tr>
  <td align = "center"><%= question.id %></td>
  <td><%= question.body %></td>
  <td><%= link_to 'Show', question_path(question) %></td>
  <td><%= link_to 'Edit', edit_question_path(question) %></td>
  <td>
      <%= link_to 'Delete',
                  question_path(question),
                  method: :delete,
                  data: { confirm: 'Are You Sure?' } %>
  </td>
</tr>
