<%=
  form_with(model: [:admin, test],
            url: update_inline_admin_test_path(test),
            class: 'form-inline',
            html: { style: 'display: none' },
            data: { test_id: test.id },
            local: true) do |form|
%>
<%= form.text_field :title, class: 'form-control mr-2' %>
<%= form.submit t('.submit'), class: 'btn btn-primary' %>

<% end %>
